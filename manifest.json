{
  "manifest_version": 3,
  "name": "Tauros 401",
  "version": "1.0",
  "description": "Transferir a HOST",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "permissions": ["tabs", "activeTab", "scripting","storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
